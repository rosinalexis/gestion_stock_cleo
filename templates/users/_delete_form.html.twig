<form method="post" action="{{ path('app_users_delete', {'id': user.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cette utilisateur ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
	<button class="my-2 btn btn-danger">Supprimer Utilisateur</button>
</form>
